#day2_3
A=c(76,35,47,64,95,66,89,36,84,76,35,47,64,95,66,89,36,84)
B=c(51,56,84,60,59,70,63,66,50,51,56,84,60,59,70,63,66,50)

#higher_mean
mean(A)
mean(B)
max((mean(A)),(mean(B)))

#higher_median
median(A)
median(B)
max((median(A)),(median(B)))

#range
(max(A)-min(A))
(max(B)-min(B))

#boxplot
boxplot(A,B)
